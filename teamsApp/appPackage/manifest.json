{
  "$schema": "https://developer.microsoft.com/en-us/json-schemas/teams/v1.23/MicrosoftTeams.schema.json",
  "manifestVersion": "1.23",
  "version": "1.0.0",
  "id": "${TEAMS_APP_ID}",
  "developer": {
    "name": "Swivel Group",
    "websiteUrl": "https://swivelgroup.com.au",
    "privacyUrl": "https://swivelgroup.com.au/privacy",
    "termsOfUseUrl": "https://swivelgroup.com.au/termsofuse"
  },
  "icons": {
    "color": "color.png",
    "outline": "outline.png"
  },
  "name": {
    "short": "Swivel Portal",
    "full": "Swivel Portal (${{APP_NAME_SUFFIX}})"
  },
  "description": {
    "short": "Swivel Portal (${{APP_NAME_SUFFIX}})",
    "full": "Swivel Portal for Microsoft Teams"
  },
  "accentColor": "#FFFFFF",
  "bots": [],
  "composeExtensions": [],
  "staticTabs": [
    {
      "entityId": "index0",
      "name": "Home",
      "contentUrl": "${{TAB_ENDPOINT}}",
      "websiteUrl": "${{TAB_ENDPOINT}}",
      "scopes": ["personal", "groupChat", "team"]
    }
  ],
  "permissions": ["identity", "messageTeamMembers"],
  "webApplicationInfo": {
    "id": "${{ENTRA_APP_ID}}",
    "resource": "${{ENTRA_APP_URI}}"
  },
  "validDomains": ["${{TAB_DOMAIN}}"]
}
